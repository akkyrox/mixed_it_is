<h1>New Physician</h1>
<%= form_for :physician, url: physicians_path do |f| %>

<p>
 <%= f.label :name %><br>
 <%= f.text_field :name %>
</p>
<p>
 <%= f.label :speciality %><br>
 <%= f.text_field :speciality %>
</p>
 
<p>
 <%= f.submit %>
</p>
<% end %>
 
<%= link_to 'Back', physicians_path %>

<%form_tag %>